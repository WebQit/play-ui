{
  "name": "@webqit/playui-element",
  "title": "Play UI JavaScript",
  "description": "Modern UI suite and web component library.",
  "keywords": [
    "webcomponents"
  ],
  "homepage": "https://webqit.io/tooling/play-ui/docs/getting-started/overview#play-ui-element",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webqit/play-ui.git"
  },
  "bugs": {
    "url": "https://github.com/webqit/play-ui/issues"
  },
  "type": "module",
  "sideEffects": false,
  "browser": {
    "fs": false
  },
  "main": "./src/index.js",
  "scripts": {
    "test": "mocha --extension .test.js --exit",
    "test:coverage": "c8 --reporter=text-lcov npm run test | coveralls",
    "build": "esbuild main=src/targets.browser.js lite=src/targets.browser.lite.js --bundle --minify --sourcemap --outdir=dist",
    "preversion": "npm run build && git add -A dist",
    "postversion": "npm publish",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "@webqit/observer": "^2.0.1",
    "@webqit/subscript": "^2.1.35",
    "@webqit/util": "^0.8.9"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "mocha": "^10.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "esbuild": "^0.15.16"
  },
  "author": {
    "name": "Oxford Harrison",
    "email": "oxharris.dev@gmail.com"
  },
  "maintainers": [
    {
      "name": "Oxford Harrison",
      "email": "oxharris.dev@gmail.com"
    }
  ],
  "contributors": [],
  "funding": {
    "type": "patreon",
    "url": "https://patreon.com/ox_harris"
  }
}
